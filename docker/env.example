# CA Gap Analyzer Docker Environment Variables
# Copy this file to .env and fill in your values

# Azure AD App Registration (for automated collection)
# Leave blank to use device code flow
CA_GAP_CLIENT_ID=
CA_GAP_CLIENT_SECRET=
CA_GAP_TENANT_ID=

# Use Azure Managed Identity (for Azure-hosted deployments)
# Set to any value to enable
CA_GAP_USE_MANAGED_IDENTITY=

# Web UI Port
WEB_PORT=8080

